    "@google/generative-ai": "^0.21.0",
    "express": "^4.21.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.23.8",
    "express-rate-limit": "^7.4.1",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "winston": "^3.17.0",
    "handlebars": "^4.7.8",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.5",
    "uuid": "^11.0.3"
